@model PaginationViewModel
@{
    var searchResults = (SearchResultsViewModel)Model.Model;

    ViewData["Title"] = "Sök - " + Model.TotalModelCount + " st resultat";
    ViewData["Message"] = "Hitta den auktion du letar efter.";
}

<partial name="_SearchPartialView" model="@searchResults.SearchModel" />
<partial name="_IndexPartialView" model="@searchResults.AuktionModel" />
<partial name="_PaginationPartialView" model="@Model.GetWithOtherModel(searchResults.SearchModel)" />

@section Scripts
{
    <partial name="_TooltipPartialView" />
    <partial name="_ValidationScriptsPartial"/>
}
